{
  "name": "DBRTableExtend",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "DBR難易度表(イージー)の機能を拡張します",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://dbr-difficulty.github.io/dbralle.html",
        "https://dbr-difficulty.github.io/dbralle_*.html"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}